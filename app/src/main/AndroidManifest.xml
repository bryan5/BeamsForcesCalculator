            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CantileverActivity"
            android:label="@string/title_activity_cantilever" >
        </activity>
        <activity
            android:name=".SimpleSupportsActivity"
            android:label="@string/title_activity_simple_supports" >
        </activity>
    </application>

</manifest>
